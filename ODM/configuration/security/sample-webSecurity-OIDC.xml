<server>
 <!-- This group have permission to log into all applications and will be have all roles.-->
 <variable name="oidcFullAccess" value="o=defaultWIMFileBasedRealm/oidcAllRoles"/>
  
 <!-- --------------------------------------------------- -->
 <!-- Give role rtsAdministrator to LDAP group oidcRtsAdministrator, basic group oidcAllRoles and LDAP user generalAdmin -->
 <!-- --------------------------------------------------- -->
 <variable name="odm.rtsAdministrators.group1" value="group:o=defaultWIMFileBasedRealm/cn=oidcRtsAdministrator,ou=memberlist,ou=ibmgroups,o=ibm.com"/>
 <variable name="odm.rtsAdministrators.group2" value="group:${oidcFullAccess}"/>
 <variable name="odm.rtsAdministrators.user1" value="uid=generalAdmin,c=fr,ou=bluepages,o=ibm.com"/>

 <!-- --------------------------------------------------- -->
